program x;
var 
numero : int;

function fibonacci( num : int) : int
var resultado : int;
{
    if (num<2)
    {
	resultado = 1;
    }

    if (num==2)
    {
	resultado = 1;
    }


    if (num > 2)
    {
     resultado = fibonacci(num-1) + fibonacci(num-2);
    }
  
ret resultado
};

begin
numero=7;
print("Resultado: ", fibonacci(numero));

end


