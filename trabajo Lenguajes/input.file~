program x;
var 
grade : char;

function suma(op1:int, op2:int) : int
	var resultado:int;
	
{
	resultado=op1+op2;
	op2=suma(1,2);
	ret resultado
};
begin

print("ENTER YOUR GRADES");
read(grade);
case (grade) of 
{
'A': print("Excellent");
'B': print("Good");
'C': print("enough");
