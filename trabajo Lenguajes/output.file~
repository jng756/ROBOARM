VAR	grade	tipo	char
FUNCTION 	suma 	int
PASS		op1	int
PASS		op2	int
ENDFUNCTION
VAR	resultado	tipo	int
+	op1	op2	 t1
=	resultado	t1
=	op2	suma(1,2)
RETURN	resultado
BEGIN
PRINT	"ENTER YOUR GRADES"
READ	grade
CASE	grade
OPTION	'A'
PRINT	"Excellent"
OPTION	'B'
PRINT	"Good"
OPTION	'C'
PRINT	"enough"
OPTION	'D'
EXTRAOPTION	'E'
PRINT	"not enough"
ENDCASE
END
